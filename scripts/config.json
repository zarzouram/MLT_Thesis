{
    "langs": [
        "ar",
        "en"
    ],
    "parsers": {
        "udp": {
            "class": "UDPipeRestParser",
            "output_converter": "UDPipeConverters",
            "params": {
                "tokenizer": "presegmented",
                "tagger": "",
                "parser": ""
            }
        },
        "stanza": {
            "class": "StanzaParser",
            "output_converter": "StanzaConverters",
            "params": {
                "common": {
                    "dir": "data/resources/stanza",
                    "verbose": false
                },
                "ar": {
                    "processors": "tokenize,mwt,pos,lemma,depparse"
                },
                "en": {
                    "processors": "tokenize,pos,lemma,depparse"
                }
            }
        }
    }
}